for k = 0, n_fit_class -1 do begin
  minxss_test_function_data_structure = minxss_x123_compute_ospex_fit(minxss_test_function_data_spex_specfile, minxss_test_function_data_spex_file_reader, minxss_fit_class_array[k]) ;, $
