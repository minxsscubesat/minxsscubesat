save, /compress, minxss_test_function_data_structure, filename = COMPUTER_PATH+'Research_LASP'+path_sep()+'MinXSS'+path_sep()+'MinXSS_Data'+path_sep()+'OSPEX'+path_sep()+'minxss1_level1_1440minute_average_mission_ospex_fit_'+minxss_fit_class_array[k]+'_0_240_keV_FWHM.sav'

