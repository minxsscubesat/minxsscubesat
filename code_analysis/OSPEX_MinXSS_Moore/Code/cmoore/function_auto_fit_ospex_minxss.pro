  n_fit_class = 6.0
  minxss_fit_class_array = strarr(n_fit_class)
  minxss_fit_class_array[0] = '1TCoronal'
  minxss_fit_class_array[1] = '2TCoronal'
  minxss_fit_class_array[2] = '1TFree'
  minxss_fit_class_array[3] = '2TFree'
  minxss_fit_class_array[4] = '1TAllFree'
  minxss_fit_class_array[5] = '2TAllFree'


