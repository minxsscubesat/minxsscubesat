  ;bk_start_date
  bk_start_date_month = 07
  bk_start_date_day = 24
  bk_start_date_year = 2016
  bk_start_date_hour = 11
  bk_start_date_minute = 05
  bk_start_date_second = 00
  bk_start_time_jd = julday(bk_start_date_month, bk_start_date_day, bk_start_date_year, bk_start_date_hour, bk_start_date_minute, bk_start_date_second)
  ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; end time = april 25, 2017, 23:59:59 ut
